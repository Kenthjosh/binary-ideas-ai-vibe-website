        <div class="glow-sphere glow-sphere--1 absolute top-[5%] left-[18%] w-72 h-72 bg-[#2b7fff] rounded-[33554400px] blur-3xl"></div>
        <div class="glow-sphere glow-sphere--2 absolute top-[18%] right-[15%] w-80 h-80 bg-[#ac46ff] rounded-[33554400px] blur-3xl"></div>
        <div class="glow-sphere glow-sphere--3 absolute top-[40%] left-1/2 w-80 h-80 -translate-x-1/2 bg-[#f6329a] rounded-[33554400px] blur-3xl"></div>
        <div class="glow-sphere glow-sphere--4 absolute top-[60%] left-[12%] w-64 h-64 bg-[#2b7fff] rounded-[33554400px] blur-3xl"></div>
        <div class="glow-sphere glow-sphere--5 absolute top-[75%] right-[20%] w-72 h-72 bg-[#ac46ff] rounded-[33554400px] blur-3xl"></div>
        <div class="glow-sphere glow-sphere--6 absolute top-[90%] left-[55%] w-64 h-64 bg-[#f6329a] rounded-[33554400px] blur-3xl"></div>
      </div>

